    <script src="<%= APP_PATH %>/scripts/services/utils.js?v=<%= APP_VERSION %>"></script>
    <script src="<%= APP_PATH %>/scripts/app.js?v=<%= APP_VERSION %>"></script> 
  </body>
</html>
